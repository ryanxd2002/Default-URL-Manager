{
    "manifest_version": 3,
    "name": "Default URL Manager",
    "version": "1.0",
    "description": "Manage and set default URLs for specific keywords.",
    "permissions": ["storage", "tabs"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      }
    },
    "omnibox": { "keyword": "go" },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
  